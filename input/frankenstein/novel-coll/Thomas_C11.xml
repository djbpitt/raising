            <seg type="invariant-MissingWit" xml:id="C11_app144-fThomas_start"/>follow. <seg xml:id="C11_app144-fThomas_end"/>His <seg xml:id="C11_app146-fThomas_start"/>constant <seg xml:id="C11_app146-fThomas_end"/>
            <seg xml:id="C11_app147-fThomas_start"/>illness <seg xml:id="C11_app147-fThomas_end"/>
