            <seg type="invariant-MissingWit" xml:id="C11_app238-fThomas_start"/>I <seg xml:id="C11_app238-fThomas_end"/>
            <seg xml:id="C11_app239-fThomas_start"/>said, <seg xml:id="C11_app239-fThomas_end"/>
            <seg type="invariant-MissingWit" xml:id="C11_app240-fThomas_start"/>that <seg xml:id="C11_app240-fThomas_end"/>
